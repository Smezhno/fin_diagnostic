# === Приложение ===
DEBUG=false

# === Лимиты данных ===
MAX_FILE_SIZE_MB=5
MAX_ROWS=100
MIN_PERIODS=3

# === LLM retry ===
LLM_MAX_RETRIES=2
LLM_TIMEOUT_SECONDS=60
